      "keywords": ["hours", "time", "open", "closed", "schedule", "when"],
      "phrases": ["business hours", "opening hours", "what time", "when are you open", "are you open"],
      "regex": "\\b(hours|time|open|closed|schedule)\\b.*",
      "priority": 2
    },
    "price": {
      "keywords": ["price", "cost", "fee", "charge", "money", "pay", "payment", "expensive", "cheap"],
      "phrases": ["how much", "what does it cost", "pricing information", "cost of"],
      "regex": "\\b(price|cost|fee|charge|money|pay)\\b.*",
      "priority": 2
    },
    "location": {
      "keywords": ["where", "location", "address", "place", "find", "directions"],
      "phrases": ["where are you", "where is", "how to find", "your location"],
      "regex": "\\bwhere\\b.*",
      "priority": 2
    },
    "weather": {
      "keywords": ["weather", "temperature", "rain", "sunny", "cloudy", "snow", "hot", "cold"],
      "phrases": ["how's the weather", "what's the weather like", "weather forecast"],
      "regex": "\\b(weather|temperature|rain|sunny)\\b.*",
      "priority": 3
    },
    "name": {
      "keywords": ["name", "called", "who are you", "your name"],
      "phrases": ["what's your name", "who are you", "what are you called"],
      "regex": "\\b(name|called)\\b.*|who\\s+are\\s+you",
      "priority": 2
    },
    "age": {
      "keywords": ["age", "old", "how long", "when were you"],
      "phrases": ["how old are you", "what's your age", "when were you created"],
      "regex": "\\b(age|old)\\b.*|how\\s+old",
      "priority": 3
    },
    "capability": {
      "keywords": ["can you", "do you", "are you able", "what can you do"],
      "phrases": ["what can you do", "what are you capable of", "can you help with"],
      "regex": "\\b(can\\s+you|do\\s+you|able)\\b.*",
      "priority": 2
    },
    "joke": {
      "keywords": ["joke", "funny", "laugh", "humor", "tell me something funny"],
      "phrases": ["tell me a joke", "make me laugh", "something funny"],
      "regex": "\\b(joke|funny|laugh)\\b.*",
      "priority": 3
    },
    "repeat": {
      "keywords": ["repeat", "again", "say that again", "come again", "pardon"],
      "phrases": ["can you repeat", "say that again", "i didn't hear", "come again"],
      "regex": "\\b(repeat|again|pardon)\\b.*",
      "priority": 2
    },
    "unclear": {
      "keywords": ["unclear", "confusing", "don't understand", "what do you mean", "huh"],
      "phrases": ["i don't understand", "that's confusing", "what do you mean", "can you clarify"],
      "regex": "\\b(unclear|confusing|understand)\\b.*",
      "priority": 2
    }
  },
  "stop_patterns": {
    "keywords": ["exit", "quit", "stop", "end", "bye", "goodbye", "leave", "close"],
    "phrases": ["i want to quit", "end conversation", "stop talking", "close chat"],
    "regex": "^(exit|quit|stop|end|bye|goodbye)\\b.*"
  },
  "default_pattern": {
    "intent": "unknown",
    "confidence": 0.0,
    "fallback": true
  },
  "pattern_config": {
    "case_sensitive": false,
    "match_partial": true,
    "min_confidence": 0.3,
    "max_matches": 3,
    "enable_fuzzy_matching": false,
    "word_boundary_required": true
  }
}