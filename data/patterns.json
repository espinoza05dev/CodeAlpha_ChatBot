      "keywords": ["contact", "phone", "email", "address", "reach", "call", "write"],
      "phrases": ["contact information", "how to reach", "get in touch", "contact details"],
      "regex": "\\b(contact|phone|email|call)\\b.*",
      "priority": 2
    },
