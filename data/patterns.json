{
  "patterns": {
    "greeting": {
      "keywords": ["hello", "hi", "hey", "good morning", "good afternoon", "good evening", "greetings", "howdy", "what's up", "sup"],
      "phrases": ["how are you", "how do you do", "nice to meet you", "good to see you"],
      "regex": "^(hi|hello|hey)\\b.*",
      "priority": 1
    },
    "goodbye": {
      "keywords": ["bye", "goodbye", "farewell", "see you", "later", "exit", "quit", "stop", "end", "leave"],
      "phrases": ["see you later", "talk to you later", "catch you later", "until next time", "have a good day", "take care"],
      "regex": "^(bye|goodbye|exit|quit)\\b.*",
      "priority": 1
    },
    "help": {
      "keywords": ["help", "assist", "support", "aid", "guide", "explain", "how", "what", "can you"],
      "phrases": ["i need help", "can you help", "help me", "i don't know", "i'm confused", "what can you do"],
      "regex": "\\b(help|assist|support)\\b.*",
      "priority": 2
    },
    "thanks": {
      "keywords": ["thank", "thanks", "appreciate", "grateful", "thx", "ty"],
      "phrases": ["thank you", "thanks a lot", "much appreciated", "i appreciate it"],
      "regex": "\\b(thank|thanks)\\b.*",
      "priority": 1
    },
    "yes": {
      "keywords": ["yes", "yeah", "yep", "sure", "ok", "okay", "alright", "correct", "right", "exactly"],
