      "regex": "^(what|where|when|why|how|who|which)\\b.*\\?",
      "priority": 3
    },
    "apologize": {
      "keywords": ["sorry", "apologize", "my bad", "oops", "mistake", "error"],
      "phrases": ["i'm sorry", "my apologies", "i apologize", "sorry about that"],
      "regex": "\\b(sorry|apologize)\\b.*",
      "priority": 2
    },
    "compliment": {
      "keywords": ["good", "great", "awesome", "excellent", "amazing", "wonderful", "fantastic", "perfect"],
      "phrases": ["you're great", "good job", "well done", "that's amazing", "you're awesome"],
      "regex": "\\b(good|great|awesome|excellent)\\b.*",
      "priority": 2
    },
    "complaint": {
      "keywords": ["bad", "terrible", "awful", "horrible", "worst", "hate", "sucks", "useless"],
      "phrases": ["this is bad", "doesn't work", "not working", "having problems", "this sucks"],
      "regex": "\\b(bad|terrible|awful|horrible|hate)\\b.*",
      "priority": 2
    },
    "small_talk": {
      "keywords": ["how are you", "how's it going", "what's new", "how have you been"],
      "phrases": ["how are you doing", "what's up with you", "how's your day", "how are things"],
      "regex": "how\\s+(are|is)\\s+you.*",
      "priority": 3
    },
