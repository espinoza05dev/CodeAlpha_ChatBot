      "regex": "^(yes|yeah|yep|ok|okay)\\b.*",
      "priority": 1
    },
    "no": {
      "keywords": ["no", "nope", "nah", "never", "not really", "incorrect", "wrong"],
      "phrases": ["i don't think so", "that's not right", "i disagree", "not really"],
      "regex": "^(no|nope|nah)\\b.*",
      "priority": 1
